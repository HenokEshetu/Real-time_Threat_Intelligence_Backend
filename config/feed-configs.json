[
  {
    "id": "misp-feed",
    "name": "MISP",
    "apiUrl": "http://localhost/events/restSearch",
    "apiKeyEnv": "MISP_API_KEY",
    "method": "POST",
    "headers": {
      "Authorization": "${apiKey}",
      "Accept": "application/json",
      "Content-Type": "application/json"
    },
    "data": {
      "returnFormat": "json",
      "withAttachments": false,
      "limit": 5,
      "published": true
    },
    "responsePath": "response",
    "indicatorMapper": "misp",
    "batchSize": 50,
    "rateLimitDelay": 3000,
    "timeout": 90000,
    "maxRetries": 5,
    "schedule": "0 */10 * * * *",
    "pagination": {
      "paramType": "data",
      "pageKey": "page",
      "maxPages": 5
    }
  },


  {
    "id": "alienvault-otx-feed",
    "name": "AlienVaultOTX",
    "apiUrl": "https://otx.alienvault.com/api/v1/pulses/subscribed",
    "apiKeyEnv": "OTX_API_KEY",
    "method": "GET",
    "headers": {
      "X-OTX-API-KEY": "${apiKey}",
      "Accept": "application/json"
    },
    "params": {
      "limit": 5
    },
    "responsePath": "results",
    "indicatorMapper": "alienVaultOTX",
    "batchSize": 50,
    "rateLimitDelay": 2000,
    "timeout": 60000,
    "maxRetries": 3,
    "schedule": "0 */10 * * * *",
    "pagination": {
      "paramType": "params",
      "pageKey": "page",
      "totalCountKey": "count",
      "maxPages": 5
    }
  },


  {
    "id": "hybrid-analysis-feed",
    "name": "HybridAnalysis",
    "apiUrl": "https://www.hybrid-analysis.com/api/v2/feed/latest",
    "apiKeyEnv": "HYBRID_ANALYSIS_API_KEY",
    "method": "GET",
    "headers": {
      "api-key": "${apiKey}",
      "User-Agent": "Falcon Sandbox",
      "Accept": "application/json"
    },
    "params": {
      "limit": 5
    },
    "responsePath": "data",
    "indicatorMapper": "hybridAnalysis",
    "batchSize": 50,
    "rateLimitDelay": 2000,
    "timeout": 60000,
    "maxRetries": 3,
    "schedule": "0 */10 * * * *",
    "pagination": {
      "paramType": "params",
      "pageKey": "page",
      "hasNextKey": "has_next",
      "maxPages": 5
    }
  }
 ]